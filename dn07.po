#. name="description"><title></title></head><body><div id="text"
#. lang="pi"><section class="sutta" id="dn7"><article><div class="hgroup"><p
#. class="division">
msgctxt "dn7:1.1"
msgid "Dīgha Nikāya 7"
msgstr ""

#. </p><h1>
msgctxt "dn7:2.1"
msgid "Jāliyasutta"
msgstr ""

#. </h1></div><p><a class="sc" id="1"></a><a class="wp" id="wp1"></a><a
#. class="pts" id="pts1.159"></a>
msgctxt "dn7:3.1"
msgid "<span class=\"evam\">Evaṃ me sutaṃ—​</span>"
msgstr ""

msgctxt "dn7:3.2"
msgid "ekaṃ samayaṃ bhagavā kosambiyaṃ viharati ghositārāme."
msgstr ""

msgctxt "dn7:3.3"
msgid "Tena kho pana samayena dve pabbajitā—"
msgstr ""

msgctxt "dn7:3.4"
msgid "muṇḍiyo ca paribbājako jāliyo ca dārupattikantevāsī yena bhagavā tenupasaṅkamiṃsu;"
msgstr ""

msgctxt "dn7:3.5"
msgid "upasaṅkamitvā bhagavatā saddhiṃ sammodiṃsu."
msgstr ""

msgctxt "dn7:3.6"
msgid "Sammodanīyaṃ kathaṃ sāraṇīyaṃ vītisāretvā ekamantaṃ aṭṭhaṃsu."
msgstr ""

msgctxt "dn7:3.7"
msgid "Ekamantaṃ ṭhitā kho te dve pabbajitā bhagavantaṃ etadavocuṃ:"
msgstr ""

msgctxt "dn7:3.8"
msgid "“kiṃ nu kho, āvuso gotama, taṃ jīvaṃ taṃ sarīraṃ, udāhu aññaṃ jīvaṃ aññaṃ sarīran”ti?"
msgstr ""

#. </p><p><a class="sc" id="2"></a>
msgctxt "dn7:4.1"
msgid "“Tena hāvuso, suṇātha sādhukaṃ manasi karotha, bhāsissāmī”ti."
msgstr ""

msgctxt "dn7:4.2"
msgid "“Evamāvuso”ti kho te dve pabbajitā bhagavato paccassosuṃ."
msgstr ""

msgctxt "dn7:4.3"
msgid "Bhagavā etadavoca: <a class=\"wp\" id=\"wp2\"></a>“"
msgstr ""

msgctxt "dn7:4.4"
msgid "idhāvuso, tathāgato loke uppajjati arahaṃ, sammāsambuddho …"
msgstr ""

msgctxt "dn7:4.5"
msgid "pe …"
msgstr ""

msgctxt "dn7:4.6"
msgid "Evaṃ kho, āvuso, bhikkhu sīlasampanno hoti."
msgstr ""

#. </p><p><a class="sc" id="3"></a><a class="pts" id="pts1.160"></a>
msgctxt "dn7:5.1"
msgid "…"
msgstr ""

msgctxt "dn7:5.2"
msgid "pe …"
msgstr ""

msgctxt "dn7:5.3"
msgid "Paṭhamaṃ jhānaṃ upasampajja viharati."
msgstr ""

msgctxt "dn7:5.4"
msgid "Yo kho, āvuso, bhikkhu evaṃ jānāti evaṃ passati, kallaṃ nu kho tassetaṃ vacanāya:"
msgstr ""

msgctxt "dn7:5.5"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vāti."
msgstr ""

msgctxt "dn7:5.6"
msgid "Yo so, āvuso, bhikkhu evaṃ jānāti evaṃ passati, kallaṃ tassetaṃ vacanāya:"
msgstr ""

msgctxt "dn7:5.7"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vāti."
msgstr ""

msgctxt "dn7:5.8"
msgid "Ahaṃ kho panetaṃ, āvuso, evaṃ jānāmi evaṃ passāmi."
msgstr ""

msgctxt "dn7:5.9"
msgid "Atha ca panāhaṃ na vadāmi:"
msgstr ""

msgctxt "dn7:5.10"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vā …"
msgstr ""

msgctxt "dn7:5.11"
msgid "pe …"
msgstr ""

#. <a class="wp" id="wp4"></a><a class="wp" id="wp3"></a>
msgctxt "dn7:5.12"
msgid "dutiyaṃ jhānaṃ …"
msgstr ""

msgctxt "dn7:5.13"
msgid "tatiyaṃ jhānaṃ …"
msgstr ""

msgctxt "dn7:5.14"
msgid "catutthaṃ jhānaṃ upasampajja viharati."
msgstr ""

msgctxt "dn7:5.15"
msgid "Yo kho, āvuso, bhikkhu evaṃ jānāti evaṃ passati, kallaṃ nu kho tassetaṃ vacanāya:"
msgstr ""

msgctxt "dn7:5.16"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vāti?"
msgstr ""

msgctxt "dn7:5.17"
msgid "Yo so, āvuso, bhikkhu evaṃ jānāti evaṃ passati kallaṃ, tassetaṃ vacanāya:"
msgstr ""

msgctxt "dn7:5.18"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vāti."
msgstr ""

msgctxt "dn7:5.19"
msgid "Ahaṃ kho panetaṃ, āvuso, evaṃ jānāmi evaṃ passāmi."
msgstr ""

msgctxt "dn7:5.20"
msgid "Atha ca panāhaṃ na vadāmi:"
msgstr ""

msgctxt "dn7:5.21"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vā …"
msgstr ""

msgctxt "dn7:5.22"
msgid "pe …"
msgstr ""

msgctxt "dn7:5.23"
msgid "ñāṇadassanāya cittaṃ abhinīharati abhininnāmeti …"
msgstr ""

msgctxt "dn7:5.24"
msgid "yo kho, āvuso, bhikkhu evaṃ jānāti evaṃ passati, kallaṃ nu kho tassetaṃ vacanāya:"
msgstr ""

msgctxt "dn7:5.25"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vāti."
msgstr ""

msgctxt "dn7:5.26"
msgid "Yo so, āvuso, bhikkhu evaṃ jānāti evaṃ passati kallaṃ tassetaṃ vacanāya:"
msgstr ""

msgctxt "dn7:5.27"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vāti."
msgstr ""

msgctxt "dn7:5.28"
msgid "Ahaṃ kho panetaṃ, āvuso, evaṃ jānāmi evaṃ passāmi."
msgstr ""

msgctxt "dn7:5.29"
msgid "Atha ca panāhaṃ na vadāmi:"
msgstr ""

msgctxt "dn7:5.30"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vā …"
msgstr ""

msgctxt "dn7:5.31"
msgid "pe …."
msgstr ""

#. </p><p><a class="sc" id="4"></a><a class="wp" id="wp5"></a>
msgctxt "dn7:6.1"
msgid "…"
msgstr ""

msgctxt "dn7:6.2"
msgid "pe …"
msgstr ""

msgctxt "dn7:6.3"
msgid "Nāparaṃ itthattāyāti pajānāti."
msgstr ""

msgctxt "dn7:6.4"
msgid "Yo kho, āvuso, bhikkhu evaṃ jānāti evaṃ passati, kallaṃ nu kho tassetaṃ vacanāya:"
msgstr ""

msgctxt "dn7:6.5"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vāti?"
msgstr ""

msgctxt "dn7:6.6"
msgid "Yo so, āvuso, bhikkhu evaṃ jānāti evaṃ passati, na kallaṃ tassetaṃ vacanāya:"
msgstr ""

msgctxt "dn7:6.7"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vāti."
msgstr ""

msgctxt "dn7:6.8"
msgid "Ahaṃ kho panetaṃ, āvuso, evaṃ jānāmi evaṃ passāmi."
msgstr ""

msgctxt "dn7:6.9"
msgid "Atha ca panāhaṃ na vadāmi:"
msgstr ""

msgctxt "dn7:6.10"
msgid "‘taṃ jīvaṃ taṃ sarīran’ti vā ‘aññaṃ jīvaṃ aññaṃ sarīran’ti vā”ti."
msgstr ""

#. </p><p><a class="sc" id="5"></a>
msgctxt "dn7:7.1"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "dn7:7.2"
msgid "Attamanā te dve pabbajitā bhagavato bhāsitaṃ abhinandunti."
msgstr ""

#. </p><p class="endsutta">
msgctxt "dn7:8.1"
msgid "Jāliyasuttaṃ niṭṭhitaṃ sattamaṃ."
msgstr ""

